{
    "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": ["/app/*"],
      "@src/*": ["/src/*"]
    },
    "include": [
    "*.ts",
    "*.d.ts",
    "*.tsx"
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "vite/client"
    ],

  }
}
